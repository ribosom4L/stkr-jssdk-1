{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es5",
    "module":"commonjs",
    "sourceMap": true,
    "declaration": true,
    "declarationDir": "dist/types",
    "outDir": "dist",
    "rootDir": "src",
    "typeRoots": [
      "node_modules/@types"
    ],
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*"
  ],
  "resolveJsonModule": true
}
